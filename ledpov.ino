void printletter(int letter[])
{
  int y;
  
  for(y=0;y<8;y++)
  {
    digitalWrite(y+2,letter[y]);
  }
  delay(dottime);
  for(y=0;y<8;y++)
  {
    digitalWrite(y+2,letter[y+8]);
  }
  delay(dottime);
  for(y=0;y<8;y++)
  {
    digitalWrite(y+2,letter[y+16]);
  }
  delay(dottime);
  for(y=0;y<8;y++)
  {
    digitalWrite(y+2,letter[y+24]);
  }
  delay(dottime);
  
  for(y=0;y<8;y++)
  {
    digitalWrite(y+2,letter[y+32]);
  }
  delay(dottime);
  for(y=0;y<8;y++){
    digitalWrite(y+2,0);
  }
  delay(letterspace);
  }
  
